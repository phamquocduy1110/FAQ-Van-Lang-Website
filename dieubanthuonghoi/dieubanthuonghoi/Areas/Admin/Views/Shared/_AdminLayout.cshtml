<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Fontfaces CSS-->
    <link href="@Url.Content("~/Assets/css/AdminIndexCss/font-face.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/font-awesome-4.7/css/font-awesome.min.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/font-awesome-5/css/fontawesome-all.min.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/mdi-font/css/material-design-iconic-font.min.css")" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/bootstrap-4.1/bootstrap.min.css")" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/animsition/animsition.min.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/wow/animate.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/css-hamburgers/hamburgers.min.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/slick/slick.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/select2/select2.min.css")" rel="stylesheet" media="all">
    <link href="@Url.Content("~/Assets/vendor/AdminIndexVendor/perfect-scrollbar/perfect-scrollbar.css")" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="@Url.Content("~/Assets/css/AdminIndexCss/theme.css")" rel="stylesheet" media="all">

</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="@Url.Content("~/Assets/images/logo/IT-ADMIN_Logo.png")" alt="Cool Admin" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a href="@Url.Action("Index", "AdminHome")">
                                <i class="fas fa-chart-bar"></i>Dashboard
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "AdminUsersRole")">
                                <i class="fas fa-user"></i>Users Role
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "ListQuestionsAdmin")">
                                <i class="fas fa-table"></i>Tables
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">
                                @Html.Partial("_AdminLoginPartial")
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            @RenderBody()
            <!-- END PAGE CONTAINER-->
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/jquery-3.2.1.min.js")"></script>
    <!-- Bootstrap JS-->
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/bootstrap-4.1/popper.min.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/bootstrap-4.1/bootstrap.min.js")"></script>
    <!-- Vendor JS       -->
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/slick/slick.min.js")">
    </script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/wow/wow.min.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/animsition/animsition.min.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/bootstrap-progressbar/bootstrap-progressbar.min.js")">
    </script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/counter-up/jquery.waypoints.min.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/counter-up/jquery.counterup.min.js")">
    </script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/circle-progress/circle-progress.min.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/perfect-scrollbar/perfect-scrollbar.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/chartjs/Chart.bundle.min.js")"></script>
    <script src="@Url.Content("~/Assets/vendor/AdminIndexVendor/select2/select2.min.js")">
    </script>

    <!-- Main JS-->
    <script src="@Url.Content("~/Assets/js/AdminIndexJs/main.js")"></script>

</body>

</html>
<!-- end document-->
