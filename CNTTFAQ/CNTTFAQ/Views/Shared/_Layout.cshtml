    <link href="@Url.Content("~/Assets/css/UserPageCss/footer.css")" rel="stylesheet" type="text/css" media="all" />


    <link rel="stylesheet" href="@Url.Content("~/Assets/css/UserPageCss/bootstrap.min.css")" />
