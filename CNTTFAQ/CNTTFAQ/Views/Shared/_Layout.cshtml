    <link rel="stylesheet" href="@Url.Content("~/Assets/css/UserPageCss/style.css")" />
</head>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader">
            <img src="@Url.Content("~/Assets/images/logo/LoadingLogo.png")" alt="">
            <h2>Đang tải.....</h2>
        </div>
    </div>

    <!-- Header section -->
    <div class="navbar navbar-inverse navbar-fixed-top" style="position: fixed; background-size:cover; background-image: url('@Url.Content("~/Assets/images/logo/navigation-bar-background.jpg")')">
        <div class="container" style="padding: 10px 0px">
            <div class="navbar-header" style="width: 250px; height: 50px; margin-right: 40px">
                <a href="@Url.Action("Index", "Home")">
                    <img src="@Url.Content("~/Assets/images/logo/VanLangCNTT_Logo.png")">
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="@Url.Action("Index", "Home")">Trang Chủ</a></li>
                    <li><a href="@Url.Action("Index", "Questions")">Câu hỏi thường gặp</a></li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <!-- Header section end -->
    @RenderBody()
    <!-- Start Footer -->
    <div class="main">
        <div class="content">
            <div class="footer-grids">
                <div class="footer one">
                    <h3>More About Company</h3>
                    <p>
                        Nemo enim ipsam voluptatem quia
